<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quiles.workout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="             Selecione seu treino"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name=".TelaPrincipal"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:parentActivityName=".TelaPrincipal"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".CriarConta"
            android:label="@string/title_activity_criar_conta"
            android:parentActivityName=".TelaPrincipal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Inicio"
            android:label="@string/title_activity_inicio"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NivelFeminino"
            android:label="       Treino feminino"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NivelMasculino"
            android:label="     Treino masculino"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Treino"
            android:label="    Selecione o treino"
            android:parentActivityName=".NivelMasculino"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Exercicios"
            android:label="    Seleciona o exercÃ­cio"
            android:parentActivityName=".Treino"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>